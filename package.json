{
    "name": "milkscript",
    "publisher": "badosz",
    "displayName": "MilkScript",
    "description": "MilkScript language support for Visual Studio Code",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/badosz0/vscode-milkscript.git"
      },
    "icon": "assets/milkscript_sygnet.png",
    "contributes": {
        "languages": [{
            "id": "milkscript",
            "aliases": ["MilkScript", "milkscript"],
            "extensions": ["milk"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "milkscript",
            "scopeName": "source.milk",
            "path": "./syntaxes/milkscript.tmLanguage.json"
        }],
        "snippets": [{
                "language": "milkscript",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}